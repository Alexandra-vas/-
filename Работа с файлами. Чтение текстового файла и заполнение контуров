#include <iostream>
#include <fstream>
#include <vector>
 
using namespace std;
 
int main() {
  int rows = 5;
  int cols = 10;
 
  vector<vector<char>> image(rows, vector<char>(cols));
 
  for (int i = 0; i < rows; ++i) {
    for (int j = 0; j < cols; ++j) {
      if (i == 0 || i == rows - 1 || j == 0 || j == cols - 1) {
        image[i][j] = '*'; 
      } else {
        image[i][j] = '.';
      }
    }
  }
 
  ofstream outputFile("image.txt");
 
  if (outputFile.is_open()) {
    for (int i = 0; i < rows; ++i) {
      for (int j = 0; j < cols; ++j) {
        outputFile << image[i][j];
      }
      outputFile << endl;
    }
    outputFile.close();
    cout << "Рисунок успешно записан в файл image.txt" << endl;
  } else {
    cerr << "Не удалось открыть файл для записи." << endl;
  }
 
  cout << "Содержимое массива:" << endl;
  for (int i = 0; i < rows; ++i) {
    for (int j = 0; j < cols; ++j) {
      cout << image[i][j];
    }
    cout << endl;
  }
 
  return 0;
}
